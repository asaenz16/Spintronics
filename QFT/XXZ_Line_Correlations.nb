         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"s1", "==", "1"}], "&&", 
            RowBox[{"s2", "==", "2"}]}], ")"}], " ", "||", " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"s1", "==", "2"}], " ", "&&", " ", 
            RowBox[{"s2", "==", "1"}]}], ")"}]}], " ", ",", " ", 
         RowBox[{"Return", "[", 
