       RowBox[{"(", 
        RowBox[{"v1", "-", "v2"}], ")"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"r", "!=", "1"}], ",", 
       RowBox[{"Return", "[", "0", "]"}]}], " ", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"dx", "=", " ", 
      RowBox[{"Norm", "[", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"v1", "-", "v2"}], ")"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"dx", "==", "1"}], ",", " ", 
